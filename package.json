{
    "name": "@byungi/lru-cache",
    "description": "Typed fast LRU cache for browser.",
    "version": "0.0.2",
    "author": "skt-t1-byungi <tiniwz@gmail.com>",
    "repository": "https://github.com/skt-t1-byungi/lru-cache.git",
    "main": "index.js",
    "types": "index.d.ts",
    "license": "MIT",
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "keywords": [
        "lru",
        "cache",
        "ttl",
        "time to live"
    ],
    "scripts": {
        "test": "npm run clean && ava",
        "clean": "rm -f *.js *.d.ts",
        "build": "tsc",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@byungi/p-delay": "^0.1.3",
        "ava": "^2.4.0",
        "eslint": "^6.6.0",
        "eslint-config-byungi": "^0.7.6",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.4"
    },
    "eslintConfig": {
        "extends": "byungi/typescript"
    },
    "eslintIgnore": [
        "index.js",
        "index.d.ts"
    ],
    "publishConfig": {
        "access": "public"
    },
    "ava": {
        "compileEnhancements": false,
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    }
}
